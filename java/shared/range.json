{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/Levertion/minecraft-json-schema/master/java/shared/range.json",
    "oneOf": [
        {
            "type": "number"
        },
        {
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "min": {
                    "type": "number"
                },
                "max": {
                    "type": "number"
                }
            }
        }
    ]
}

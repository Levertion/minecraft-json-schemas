{
    "$schema": "http://json-schema.org/schema#",
    "json-component": {
        "anyOf":[
            {
                "type": "object",
                "oneOf": [
                    {
                        "properties": {
                            "text": {
                                "type": "string"
                            }
                        },
                        "required":["text"]
                    },
                    {
                        "properties": {
                            "translate": {
                                "type": "string"
                            },
                            "with": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/json-component"
                                }
                            }
                        },
                        "required": ["translate"]
                    },
                    {
                        "properties": {
                            "score": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "objective": {
                                        "type": "string"
                                    },
                                    "value": {
                                        "type": [
                                            "number",
                                            "string",
                                            "boolean"
                                        ]
                                    }
                                }
                            }
                        },
                        "required":["score"]
                    },
                    {
                        "properties": {
                            "selector": {
                                "type": "string",
                                "pattern": "^@[parse]"
                            }
                        },
                        "required": ["selector"]
                    },
                    {
                        "properties":{
                            "keybind":{
                                "type":"string"
                            }
                        },
                        "required":["keybind"]
                    }
                ],
                "properties": {
                    "extra": {
                        "type": "array",
                        "$ref": "#/json-component"
                    },
                    "color": {
                        "enum": [
                            "black",
                            "dark_blue",
                            "dark_green",
                            "dark_aqua",
                            "dark_red",
                            "dark_purple",
                            "gold",
                            "gray",
                            "dark_gray",
                            "blue",
                            "green",
                            "aqua",
                            "red",
                            "light_purple",
                            "yellow",
                            "white",
                            "reset"
                        ]
                    },
                    "bold": {
                        "type": "boolean"
                    },
                    "italic": {
                        "type": "boolean"
                    },
                    "underlined": {
                        "type": "boolean"
                    },
                    "strikethrough": {
                        "type": "boolean"
                    },
                    "obfuscated": {
                        "type": "boolean"
                    },
                    "insertion": {
                        "type": "string"
                    },
                    "clickEvent": {
                        "type": "object",
                        "properties": {
                            "action": {
                                "enum": [
                                    "open_url",
                                    "run_command",
                                    "change_page",
                                    "suggest_command"
                                ]
                            }
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "hoverEvent":{
                        "anyOf":[
                            {
                                "type":"object",
                                "properties":{
                                    "action":{
                                        "type":"string",
                                        "value":"show_text"
                                    },
                                    "value":{
                                        "anyOf":[
                                            {
                                                "$ref":"#/json-component"
                                            }
                                        ]                           
                                    }
                                }
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "action": {
                                        "enum": ["show_item","show_entity"]
                                    }
                                }
                            }
                        ]
                    }
                }
            },
            {
                "type":"string"
            }
        ]
    },
